<div>
    <table class="table">
        <tr>
            <th class="text-center">Titulo</th>
            <th class="text-center">Precio</th>
            <th class="text-center">Imagen</th>
        </tr> 
        {{#each products}}
            <tr>
                <td class="text-center">{{this.product_name}}</td>
                <td class="text-center">{{this.product_price}}</td>
                <td class="text-center"><img src={{this.product_thumbnail}}></td>
            </tr>
        {{/each}}
    </table>
</div>